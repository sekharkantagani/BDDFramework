[32m+[m[32mpackage com.orangehrm.utils;[m
[32m+[m
[32m+[m[32mimport java.io.FileInputStream;[m
[32m+[m[32mimport java.io.FileNotFoundException;[m
[32m+[m[32mimport java.io.IOException;[m
[32m+[m[32mimport java.util.Properties;[m
[32m+[m
[32m+[m[32mpublic class CommonUtilities {[m
[32m+[m[41m	[m
[32m+[m	[32mpublic String getProperty(String key) {[m
[32m+[m[41m		[m
[32m+[m		[32mFileInputStream fis1=null;[m
[32m+[m		[32mtry {[m
[32m+[m			[32mfis1=new FileInputStream("src\\test\\resources\\properties\\Application.properties");[m
[32m+[m		[32m} catch (FileNotFoundException e) {[m
[32m+[m			[32me.printStackTrace();[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m		[32mProperties prop= new Properties();[m
[32m+[m[41m		[m
[32m+[m		[32mtry {[m
[32m+[m			[32mprop.load(fis1);[m
[32m+[m		[32m} catch (IOException e) {[m
[32m+[m			[32m// TODO Auto-generated catch block[m
[32m+[m			[32me.printStackTrace();[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m		[32mString value=prop.getProperty(key);[m
[32m+[m[41m		[m
[32m+[m		[32mreturn value;[m
[32m+[m[41m		[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m}[m
