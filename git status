[32m+[m		[32mif(driver==null) {[m
[32m+[m[41m			[m
[32m+[m			[32mString browserName=common.getProperty("browser");[m
[32m+[m[41m			[m
[32m+[m			[32mswitch (browserName) {[m
[32m+[m[41m			[m
[32m+[m			[32mcase "chrome":[m
[32m+[m				[32mdriver=new ChromeDriver();[m
[32m+[m[41m				[m
[32m+[m				[32mbreak;[m
[32m+[m			[32mcase "edge":[m
[32m+[m				[32mdriver=new EdgeDriver();[m
[32m+[m				[32mbreak;[m
[32m+[m			[32mcase "Firefox":[m
[32m+[m[41m				[m
[32m+[m				[32mdriver=new FirefoxDriver();[m
[32m+[m				[32mbreak;[m
[32m+[m[41m				[m
[32m+[m
[32m+[m			[32mdefault:[m
[32m+[m[41m				[m
[32m+[m				[32mtry {[m
[32m+[m					[32mthrow new InvalidBrowserException();[m
[32m+[m				[32m} catch (InvalidBrowserException e) {[m
[32m+[m					[32mSystem.out.println(e.getMessage());[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m[41m				[m
[32m+[m				[32mbreak;[m
[32m+[m[41m				[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
