[32m+[m[32mimport org.openqa.selenium.By;[m
[32m+[m[32mimport org.openqa.selenium.WebDriver;[m
[32m+[m[32mimport org.openqa.selenium.WebElement;[m
[32m+[m[32mimport org.openqa.selenium.support.ui.ExpectedConditions;[m
[32m+[m[32mimport org.openqa.selenium.support.ui.WebDriverWait;[m
[32m+[m
[32m+[m[32mimport com.orangehrm.base.BaseTest;[m
[32m+[m
[32m+[m[32mpublic class BasePage extends BaseTest {[m
[32m+[m[41m	[m
[32m+[m	[32mWebDriver driver;[m
[32m+[m[41m	[m
[32m+[m	[32mHashMap<String, By> objectRepo=new HashMap<String, By>();[m
[32m+[m[41m	[m
[32m+[m	[32mpublic BasePage(WebDriver driver) {[m
[32m+[m[41m		[m
[32m+[m		[32mthis.driver=driver;[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32m public void addObject(String logicalName, By by) {[m
[32m+[m[41m		 [m
[32m+[m		[32m objectRepo.put(logicalName, by);[m
[32m+[m[41m		 [m
[32m+[m[41m		 [m
[32m+[m[41m		 [m
[32m+[m	[32m }[m
[32m+[m[41m	 [m
[32m+[m	[32m public void type(String logicalName, String text) {[m
[32m+[m[41m		 [m
[32m+[m		[32m// driver.findElement(By.xpath("")).sendKeys(null);[m
[32m+[m[41m		 [m
[32m+[m		[32m getElement(logicalName).sendKeys(text);[m
[32m+[m	[32m }[m
[32m+[m[41m	 [m
[32m+[m	[32m public void click(String logicalName)[m
[32m+[m		[32m{[m
[32m+[m			[32mgetElement(logicalName).click();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m private WebElement getElement(String logicalName) {[m
[32m+[m[41m		[m
[32m+[m		[32m By by=objectRepo.get(logicalName);[m
[32m+[m[41m		 [m
[32m+[m		[32m waitForElementPresence(by);[m
[32m+[m		[32m WebElement element=driver.findElement(by);[m
[32m+[m				[32m return element;[m
[32m+[m[41m		 [m
[32m+[m[41m		[m
[32m+[m	[32m }[m
[32m+[m[41m	 [m
[32m+[m[41m	 [m
[32m+[m
[32m+[m	[32m private void waitForElementPresence(By by) {[m
[32m+[m		[32m WebDriverWait myWait=new WebDriverWait(driver, Duration.ofSeconds(Long.parseLong((common.getProperty("explicitWait")))));[m
[32m+[m		[32m myWait.until(ExpectedConditions.presenceOfElementLocated(by));[m
[32m+[m[41m		[m
[32m+[m	[32m }[m
[32m+[m[41m	[m
[32m+[m	[32m public void validateTitle(String expTitle)[m
[32m+[m		[32m{[m
[32m+[m[41m			[m
[32m+[m		[32m String actualTitle = driver.getTitle();[m
[32m+[m		[32m    System.out.println("Expected Title: " + expTitle);[m
[32m+[m		[32m    System.out.println("Actual Title: " + actualTitle);[m
[32m+[m		[32m    Assert.assertTrue("Title did not match. Actual: " + actualTitle, actualTitle.equalsIgnoreCase(expTitle));		}[m
[32m+[m[41m	[m
[32m+[m[32m}[m
